#include <bits/stdc++.h>
using namespace std;

int main() {
    int num, highest, position;
    cin >> highest;
    position = 1;

    for (int i = 2; i <= 100; i++) {
        cin >> num;
        if (num > highest) {
            highest = num;
            position = i;
        }
    }

    cout << highest << endl;
    cout << position << endl;

    return 0;
}
