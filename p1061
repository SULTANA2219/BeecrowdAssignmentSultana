#include <bits/stdc++.h>
#include <iomanip>
using namespace std;

int main() {
    int startDay, endDay;
    int sh, sm, ss, eh, em, es;
    string temp;

    cin >> temp >> startDay;
    cin >> sh >> temp >> sm >> temp >> ss;
    cin >> temp >> endDay;
    cin >> eh >> temp >> em >> temp >> es;

    long startSec = startDay * 24 * 3600 + sh * 3600 + sm * 60 + ss;
    long endSec   = endDay * 24 * 3600 + eh * 3600 + em * 60 + es;

    long diff = endSec - startSec;

    int days = diff / 86400; diff %= 86400;
    int hours = diff / 3600; diff %= 3600;
    int minutes = diff / 60;
    int seconds = diff % 60;

    cout << days << " dia(s)" << endl;
    cout << hours << " hora(s)" << endl;
    cout << minutes << " minuto(s)" << endl;
    cout << seconds << " segundo(s)" << endl;

    return 0;
}
