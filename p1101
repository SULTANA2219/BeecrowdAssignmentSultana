#include <bits/stdc++.h>
using namespace std;

int main() {
    while (true) {
        int M, N;
        cin >> M >> N;

        if (M <= 0 || N <= 0) break;

        int start = min(M, N);
        int end = max(M, N);
        int sum = 0;

        for (int i = start; i <= end; i++) {
            cout << i << " ";
            sum += i;
        }

        cout << "Sum=" << sum << endl;
    }

    return 0;
}
